<script>
(function(){
  const scroller = document.getElementById('DegreeScroller');
  const leftBtn  = document.querySelector('.degree-left');
  const rightBtn = document.querySelector('.degree-right');
  const pills    = scroller ? scroller.querySelectorAll('.degree-pill') : [];

  // Active state from URL
  function setActive(){
    const path = location.pathname.toLowerCase();
    pills.forEach(a=>{
      const href = (a.getAttribute('href')||'').toLowerCase();
      a.classList.toggle('active', href && path.startsWith(href));
    });
  }

  // Arrow enable/disable
  function updateArrows(){
    if(!scroller || !leftBtn || !rightBtn) return;
    const max = scroller.scrollWidth - scroller.clientWidth;
    leftBtn.disabled  = scroller.scrollLeft <= 2;
    rightBtn.disabled = scroller.scrollLeft >= (max - 2);
  }

  // Scroll handlers
  function scrollByX(x){ scroller.scrollBy({left:x, behavior:'smooth'}); }

  if (scroller){
    scroller.addEventListener('scroll', updateArrows);
    window.addEventListener('resize', updateArrows);
    updateArrows();
  }
  if (leftBtn)  leftBtn.addEventListener('click', ()=>scrollByX(-260));
  if (rightBtn) rightBtn.addEventListener('click', ()=>scrollByX(260));

  // Keyboard support: arrow keys when focused on a pill
  pills.forEach((p, i)=>{
    p.setAttribute('tabindex','0');
    p.addEventListener('keydown', (e)=>{
      if(e.key==='ArrowRight'){ e.preventDefault(); (pills[i+1]||pills[0]).focus(); }
      if(e.key==='ArrowLeft'){  e.preventDefault(); (pills[i-1]||pills[pills.length-1]).focus(); }
    });
  });

  setActive();
})();
</script>



