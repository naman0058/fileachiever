<style>
  /* ===== Degree Pills Bar ===== */
.degree-pills-wrap{
  position: relative;
  margin-top: .5rem;
  padding: .5rem 0;
}

/* scroller */
.degree-scroller{
  display: flex;
  gap: .6rem;
  overflow-x: auto;
  -webkit-overflow-scrolling: touch;
  scrollbar-width: none;  /* Firefox */
  padding: .25rem .25rem;
  scroll-snap-type: x proximity;
  border-radius: 14px;
  background: linear-gradient(180deg,#fff, #fff);
  box-shadow: 0 2px 12px rgba(17,24,39,.06);
  border: 1px solid var(--border, #E8EAED);
}
.degree-scroller::-webkit-scrollbar{ display:none; }

/* edge fades for a premium feel */
.degree-pills-wrap::before,
.degree-pills-wrap::after{
  content:"";
  position:absolute;
  top:0; bottom:0;
  width:40px;
  pointer-events:none;
}
.degree-pills-wrap::before{
  left:0;
  background: linear-gradient(to right, #fff, rgba(255,255,255,0));
}
.degree-pills-wrap::after{
  right:0;
  background: linear-gradient(to left, #fff, rgba(255,255,255,0));
}

/* pill */
.degree-pill{
  scroll-snap-align: start;
  display:inline-flex;
  align-items:center;
  justify-content:center;
  padding: .56rem 1rem;
  border-radius: 999px;                /* fully rounded */
  background: #F6F8FA;
  color:#111827;
  text-decoration:none;
  font-weight:600;
  font-size:.93rem;
  border:1px solid transparent;
  transition: background .15s, color .15s, border-color .15s, box-shadow .15s, transform .06s;
  white-space:nowrap;
}
.degree-pill:hover{
  background:#EEF2F7;
}
.degree-pill:active{
  transform: translateY(1px);
}
.degree-pill:focus{
  outline: none;
  box-shadow: 0 0 0 3px rgba(32,99,255,.20);
}
.degree-pill.active{
  background:#fff;
  color: var(--brandBlue, #2063FF);
  border-color: var(--brandBlue, #2063FF);
  box-shadow: 0 2px 10px rgba(32,99,255,.15);
}

/* arrows (hidden on small screens, appear on md+) */
.degree-arrow{
  position:absolute;
  top:50%; transform: translateY(-50%);
  width:36px; height:36px;
  border-radius: 999px;
  border:1px solid var(--border, #E8EAED);
  background:#fff;
  display:none;        /* default hidden */
  place-items:center;
  box-shadow:0 4px 14px rgba(17,24,39,.08);
  cursor:pointer;
}
.degree-left{  left: .25rem; }
.degree-right{ right: .25rem; }
.degree-arrow:disabled{ opacity:.45; cursor:not-allowed; }

/* show arrows on >=992px (Bootstrap lg) */
@media (min-width: 992px){
  .degree-arrow{ display:grid; }
  .degree-scroller{ padding: .25rem 2.75rem; } /* room for arrows */
}

</style>


