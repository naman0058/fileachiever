 <style>
  .contact-section {
    display: flex;
    align-items: center;
    justify-content: center;
    padding: 80px 20px;
    background-color: #fff;
  }

  .contact-card {
    max-width: 100%;
    background-color: #ffffff;
    padding: 40px;
    border-radius: 12px;
    box-shadow: 0 4px 20px rgba(0, 0, 0, 0.05);
    text-align: center;
  }

  .contact-card h1 {
    color: #007bff;
    font-size: 32px;
    margin-bottom: 20px;
  }

  .contact-card p {
    font-size: 18px;
    margin-bottom: 30px;
    line-height: 1.6;
    color: #555;
  }

  .whatsapp-btn {
    display: inline-block;
    background-color: #25D366;
    color: white;
    padding: 14px 28px;
    font-size: 16px;
    font-weight: 600;
    border: none;
    border-radius: 8px;
    text-decoration: none;
    transition: background-color 0.3s ease;
    box-shadow: 0 2px 6px rgba(0, 0, 0, 0.1);
  }

  .whatsapp-btn:hover {
    background-color: #1ebe5d;
  }

  @media (max-width: 480px) {
    .contact-card h1 { font-size: 24px; }
    .contact-card p { font-size: 16px; }
    .whatsapp-btn { font-size: 14px; padding: 12px 24px; }
  }
</style>

