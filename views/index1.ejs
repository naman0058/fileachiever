<!-- ======================= JS (tabs + sync) ======================= -->
<script>
/* Keep mobile tabs and desktop select in sync; ensure form submits correct "type" */
(function(){
  const tabs = document.querySelectorAll('.search-type-tabs .tab');
  const hidden = document.getElementById('type-hidden');
  const select = document.getElementById('hero-type');

  // Tab click -> update hidden + select
  tabs.forEach(btn => {
    btn.addEventListener('click', () => {
      tabs.forEach(b => { b.classList.remove('is-active'); b.setAttribute('aria-selected','false'); });
      btn.classList.add('is-active'); btn.setAttribute('aria-selected','true');
      const val = btn.getAttribute('data-type');
      if(hidden) hidden.value = val;
      if(select) select.value = val;
    });
  });

  // Select change -> update hidden + tabs
  select?.addEventListener('change', () => {
    const val = select.value;
    if(hidden) hidden.value = val;
    tabs.forEach(b => {
      const active = b.getAttribute('data-type') === val;
      b.classList.toggle('is-active', active);
      b.setAttribute('aria-selected', active ? 'true' : 'false');
    });
  });

  // Keyboard niceties: "/" focus search, "Esc" clear
  const q = document.getElementById('hero-q');
  window.addEventListener('keydown', (e) => {
    if(e.key === '/' && !e.metaKey && !e.ctrlKey && !e.altKey){
      e.preventDefault(); q?.focus(); q?.select();
    }
    if(e.key === 'Escape' && document.activeElement === q){ q.value = ''; }
  });
})();
</script>









