<script>
  // Utility: pick first N
  const take = (arr, n=8) => (Array.isArray(arr) ? arr.slice(0, n) : []);

  // Utility: robust accessors
  const text = (v, d='') => (v == null ? d : String(v));
  const has = (obj, key) => Object.prototype.hasOwnProperty.call(obj || {}, key);

  // Detect language/stack in a forgiving way
  function detectLang(item){
    const t1 = (item.language || item.lang || item.stack || '').toString().toLowerCase();
    const t2 = (item.tags || '').toString().toLowerCase();
    const t3 = (item.name || '').toString().toLowerCase();
    return [t1, t2, t3].join(' ');
  }

  // Card builders
  function cardReport(p){
    const name = text(p.name);
    const url  = `/${text(p.seo_name) ? 'final-year-project-report-' + p.seo_name : '#'}`;
    const desc = text(p.short_description || p.description || 'High-quality academic project documentation.');
    return `
      <div class="md-item">
        <span class="md-chip"><i class="feather-trending-up"></i> Trending</span>
        <h4 class="md-title">${name}</h4>
        <p class="md-desc">${desc}</p>
        <div class="md-actions">
          <a class="md-btn md-btn-light" href="${url}" aria-label="Open ${name} report">View Report</a>
        </div>
      </div>`;
  }



   function degreeReport(p, category) {
  const name = text(p.name);
 const url = text(p.seo_name) 
  ? `/${category ? category + '-' : ''}final-year-project-report-${p.seo_name}` 
  : '#';
  const desc = text(p.short_description || p.description || 'High-quality academic project documentation.');
  return `
    <div class="md-item">
      <span class="md-chip"><i class="feather-trending-up"></i> ${category.toUpperCase() || 'Trending'} Project Report</span>
      <h4 class="md-title">${name}</h4>
      <p class="md-desc">${desc}</p>
      <div class="md-actions">
        <a class="md-btn md-btn-light" href="${url}" aria-label="Open ${name} report">View Report</a>
      </div>
    </div>`;
}


  function cardDemo(s){
    const name = text(s.name);
    const url  = text(s.demo_url || '#');
    const desc = text(s.description || 'Preview the working app in your browser before you pay.');
    return `
      <div class="md-item">
        <span class="md-chip"><i class="feather-play-circle"></i> Live</span>
        <h4 class="md-title">${name}</h4>
        <p class="md-desc">${desc}</p>
        <div class="md-actions">
          <a class="md-btn md-btn-light" href="${url}" target="_blank" rel="noopener" aria-label="Live demo ${name}">Open Live Demo</a>
        </div>
      </div>`;
  }

  function cardSource(s){
    const name = text(s.name);
    const category = text(s.category);

    const desc = text(s.description || 'Clean frontend, backend, and database with README.');
    const url  = `/${text(s.seo_name)}/source-code`;
    return `
      <div class="md-item">
        <span class="md-chip"><i class="feather-code"></i>${category} Source Code</span>
        <h4 class="md-title">${name}</h4>
        <p class="md-desc">${desc}</p>
        <div class="md-actions">
          <a class="md-btn md-btn-light" href="${url}" aria-label="Open ${name} source code">View Details</a>
        </div>
      </div>`;
  }

  // Generic strip renderer
  function renderStrip(containerId, items, builder){
    const el = document.getElementById(containerId);
    if(!el) return;
    el.innerHTML = take(items, 8).map(builder).join('');
  }


  function degreeRenderStrip(containerId, items, category, builder) {
  const el = document.getElementById(containerId);
  if (!el) return;
  el.innerHTML = take(items, 8).map(p => builder(p, category)).join('');
}

  // Filters
  function filterPHP(list){
    return (list || []).filter(i => /php|laravel|codeigniter|mysql/i.test(detectLang(i)));
  }
  function filterNode(list){
    return (list || []).filter(i => /mern|nodejs|node/i.test(detectLang(i)));
  }
  function filterPython(list){
    return (list || []).filter(i => /python|nodejs|nestjs/i.test(detectLang(i)));
  }

   function filterMachine(list){
    return (list || []).filter(i => /machine-learning|machine|nestjs/i.test(detectLang(i)));
  }

  // Render all strips
  (function initStrips(){
    try{
      const P = window.FM?.projects || [];
      const S = window.FM?.source   || [];
      const D = window.FM?.demos    || [];

      renderStrip('StripTrendingReports', P, cardReport);
      renderStrip('StripLiveDemos',     D, cardDemo);
      renderStrip('StripPHP',           filterPHP(S),  cardSource);
      renderStrip('StripNode',          filterNode(S), cardSource);
      renderStrip('StripPython',          filterPython(S), cardSource);
      renderStrip('StripMachine',          filterMachine(S), cardSource);
      degreeRenderStrip('StripBCAReports', P,'bca', degreeReport);



    }catch(e){ console.error('Strip render error:', e); }
  })();
</script>



